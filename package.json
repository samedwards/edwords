{
  "name": "wordle",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:clean": "rimraf ./build",
    "build:styles": "babel-node --extensions \".ts\" ./scripts/compile-styles.ts",
    "build:webpack": "webpack --config ./webpack/deploy.webpack.ts",
    "build": "yarn build:clean && mkdir build && npm-run-all -p build:webpack build:styles",
    "dev:run": "npm-run-all -p dev:styles dev:webpack",
    "dev:styles": "sane \"yarn build:styles\" --glob='src/**/*.css' --watchman --wait=2",
    "dev:webpack": "webpack server --config ./webpack/local.webpack.ts --port 3000 --open",
    "dev": "rimraf ./build && mkdirp ./build && yarn dev:run",
    "lint:es:fix": "eslint ./src --fix",
    "lint:es": "eslint ./src",
    "lint:tsc": "tsc || exit 1",
    "lint": "npm-run-all -p lint:es lint:tsc"
  },
  "dependencies": {
    "@tailwindcss/ui": "^0.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.5",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.3.0",
    "seedrandom": "^3.0.1",
    "tailwindcss": "^2.0.3",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/register": "^7.12.10",
    "@types/classnames": "^2.2.11",
    "@types/lodash": "^4.14.168",
    "@types/mini-css-extract-plugin": "^1.2.2",
    "@types/postcss-import": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.18",
    "@types/react-router-dom": "^5.1.7",
    "@types/seedrandom": "^3.0.1",
    "@types/tailwindcss": "^2.0.2",
    "@types/webpack": "^4.41.25",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "autoprefixer": "^10.2.4",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.1",
    "mkdirp": "^1.0.4",
    "ncp": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.6",
    "postcss-import": "^14.0.2",
    "prettier": "^2.2.1",
    "react-svg-loader": "^3.0.3",
    "rimraf": "^3.0.2",
    "sane": "^4.1.0",
    "url-loader": "^4.1.1",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  }
}
